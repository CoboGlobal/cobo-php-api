{
  "name": "cobo/cobo_custody",
  "description": "Cobo Custody php sdk",
  "type": "library",
  "require": {
    "php": ">=7.0",
    "simplito/elliptic-php": "1.0.6",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "config": {
    "optimize-autoloader": true,
    "minimum-stability": "stable"
  },
  "autoload": {
    "classmap": [
      "Client.php",
      "ApiSigner.php",
      "LocalSigner.php",
      "Config.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "~6"
  }
}